{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
    "prettier/prettier": "error",
    "indent": ["error", 4],
    "quotes": ["error", "double", "single"],
    "semi": ["error", "always"]
    }
}
